<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true">
  <local-check-config name="LateralChecks" location="checkstyle.xml" type="project" description="Checkstyle checker designed for LateralGM and the latest Girard format">
